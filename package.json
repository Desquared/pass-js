{
  "name": "@walletpass/pass-js",
  "description": "Apple Wallet Pass generating and pushing updates from Node.js",
  "author": "Konstantin Vyatkin <tino@vtkn.io>",
  "version": "6.9.8",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/walletpass/pass-js/blob/master/MIT-LICENSE"
    }
  ],
  "engines": {
    "node": ">=10.10"
  },
  "bin": {
    "passkit-keys": "./bin/passkit-keys"
  },
  "files": [
    "build"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "test": "node -r env-app-yaml/config --expose-gc node_modules/jest/bin/jest --detectOpenHandles --logHeapUsage --maxWorkers=1",
    "lint": "eslint \"{src,__tests__}/**/*.ts\" --ignore-pattern \"*test*\" --ignore-path .gitignore",
    "postversion": "git push origin master --follow-tags",
    "prepublish": "npm run build",
    "costs": "npx cost-of-modules --no-install",
    "build": "rm -rf build & tsc --project tsconfig.build.json"
  },
  "keywords": [
    "apple",
    "wallet",
    "pass",
    "passkit",
    "iOS",
    "generating",
    "APN"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/walletpass/pass-js.git"
  },
  "dependencies": {
    "buffer-crc32": "0.2.13",
    "color-name": "1.1.4",
    "do-not-zip": "1.0.0",
    "event-iterator": "2.0.0",
    "imagesize": "1.0.0",
    "node-forge": "1.3.1",
    "strip-json-comments": "4.0.0",
    "yauzl": "2.10.0"
  },
    "devDependencies": {
    "@types/buffer-crc32": "0.2.0",
    "@types/color-name": "1.1.1",
    "@types/jest": "29.5.8",
    "@types/node": "^17.0.21",
    "@types/node-forge": "1.0.1",
    "@types/yauzl": "2.9.2",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "@typescript-eslint/typescript-estree": "5.62.0",
    "env-app-yaml": "1.0.0",
    "eslint": "8.46.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-extended": "2.0.0",
    "jest-junit": "13.0.0",
    "lint-staged": "12.5.0",
    "typescript": "5.2.2"
  }
}
